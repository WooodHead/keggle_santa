#--- $Id: make.linux.x86_64.intel.opt,v 1.4 2014/01/12 11:07:04 bzfkocht Exp $
CC		=	icc
CPPFLAGS	+=	-DNDEBUG -DNO_MSHELL
CFLAGS		=	-O3 -ip -fp-model precise -w1 -par_report0 -vec_report0 \
			-wd1173
LDFLAGS         =       -lgmp -lm

ifeq ($(ZLIB),true)
LDFLAGS		+=	-lz
endif

ifeq ($(LINK),shared)
LINK		=	normal
endif

ifeq ($(LINK),static)
LDFLAGS		+=	-static
endif
